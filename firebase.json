{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "y"
  },
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-east1"
    }
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "firestore": {
      "port": 9098
    },
    "storage": {
      "port": 9097
    },
    "ui": {
      "enabled": true,
      "port": 9090
    },
    "singleProjectMode": true,
    "hosting": {
      "port": 9096
    }
  },
  "extensions": {}
}
